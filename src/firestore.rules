rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    // DEVELOPMENT-ONLY RULES
    // Allows any authenticated user to read and write to the entire database.
    // This removes all security restrictions to speed up initial development.
    // WARNING: DO NOT use these rules in a production environment.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
